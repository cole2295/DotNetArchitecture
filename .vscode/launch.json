{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "AspNetCoreAngular",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/Web/AspNetCoreAngular/bin/Debug/netcoreapp2.0/Solution.Web.AspNetCoreAngular.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Web/AspNetCoreAngular",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": true,
                "args": "${auto-detect-url}",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start ${auto-detect-url}"
                }
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        }
    ]
}